{
    "name": "development",
    "version": "2.0.0",
    "description": "Development environment management for PHP, Node.js, Python, and other languages",
    "author": "Linux Manager Team",
    "dependencies": ["core", "ui", "packages", "event"],
    "provides": ["php_manager", "nodejs_manager", "python_manager", "dev_tools"],
    "api_version": "2.0",
    "health_check": "check_development_health",
    "events": {
        "subscribes": ["system.ready", "packages.installed", "ui.theme_changed"],
        "publishes": ["dev.environment_changed", "dev.tool_installed", "dev.version_switched"]
    },
    "permissions": ["sudo"],
    "config": {
        "auto_detect_versions": true,
        "preferred_php_version": "8.2",
        "preferred_node_version": "lts",
        "install_global_packages": true,
        "dev_cache_enabled": true,
        "parallel_builds": true
    }
}
